{
  "name": "lodestar-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-app-rewired start",
    "build": "CI=false env-cmd -f .env.${NODE_ENV:-production} react-app-rewired build",
    "test": "react-app-rewired test",
    "typegen": "apollo client:codegen ./src/hasura.d.ts --outputFlat --target typescript",
    "format:organize-imports": "organize-imports-cli ./src/**/*.ts[x]",
    "format:prettier": "prettier --write \"./src/**/*.ts[x]\"",
    "format": "npm run format:organize-imports && npm run format:prettier",
    "trans:compile": "tsc --noEmit false --outDir src/extracted",
    "trans:extract": "babel --config-file ./.intl.babelrc ./src/extracted > /dev/null",
    "trans:manage": "node manageTranslations.js",
    "trans:clean": "rm -rf ./src/extracted",
    "trans": "npm run trans:clean && npm run trans:compile && npm run trans:extract && npm run trans:manage && npm run trans:clean"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
